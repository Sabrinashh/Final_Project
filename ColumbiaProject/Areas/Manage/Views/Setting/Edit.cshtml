@model Setting


<h1>Edit</h1>

<div class="container-fluid w-75">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-8">
                <form asp-action="edit" method="post">
                    <div class="form-group">
                        <label asp-for="Key">Key</label>
                        <input asp-for="Key" type="text" class="form-control" id="nameInput">
                        <span class="text-danger" asp-validation-for="Key"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Value">Value</label>
                        <input asp-for="Value" type="text" class="form-control" id="nameInput">
                        <span class="text-danger" asp-validation-for="Value"></span>
                    </div>
                   
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>